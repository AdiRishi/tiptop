{
  "name": "editor-webapp",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build --sourcemap",
    "dev": "remix dev --manual -c \"pnpm run preview\"",
    "preview": "wrangler pages dev --compatibility-date=2023-09-14 ./public --compatibility-flags=nodejs_compat",
    "typecheck": "tsc --incremental",
    "lint": "eslint . --ext .ts,.cjs,.mjs,.tsx,.jsx --fix",
    "codegen:graphql": "graphql-codegen --config graphql-codegen.ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@cloudflare/pages-plugin-sentry": "^1.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@sentry/remix": "^7.85.0",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "isbot": "^3.7.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-development-tools": "^3.5.1",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "graphql-scalars": "^1.22.4",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "wrangler": "^3.18.0"
  }
}
